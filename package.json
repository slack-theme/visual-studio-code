{
    "name": "slack-theme",
    "displayName": "Slack Theme",
    "description": "Color scheme inspired by Slack's default themes",
    "version": "1.9.14",
    "publisher": "felipe-mendes",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "slack",
        "theme",
        "light",
        "dark"
    ],
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#420D3F",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/felipemendes/slack-theme"
    },
    "contributes": {
        "themes": [
            {
                "label": "Slack Theme Aubergine - NEW COLOR",
                "uiTheme": "vs",
                "path": "./themes/aubergine-new.json"
            },
            {
                "label": "Slack Theme Aubergine",
                "uiTheme": "vs-dark",
                "path": "./themes/aubergine.json"
            },
            {
                "label": "Slack Theme Aubergine Dark",
                "uiTheme": "vs-dark",
                "path": "./themes/aubergine-dark.json"
            },
            {
                "label": "Slack Theme Monument",
                "uiTheme": "vs-dark",
                "path": "./themes/monument.json"
            },
            {
                "label": "Slack Theme Ochin",
                "uiTheme": "vs",
                "path": "./themes/ochin.json"
            },
            {
                "label": "Slack Theme Work Hard",
                "uiTheme": "vs-dark",
                "path": "./themes/work-hard.json"
            },
            {
                "label": "Slack Theme Choco Mint",
                "uiTheme": "vs-dark",
                "path": "./themes/choco-mint.json"
            },
            {
                "label": "Slack Theme Hoth",
                "uiTheme": "vs",
                "path": "./themes/hoth.json"
            },
            {
                "label": "Slack Theme Protanopia & Deuteranopia",
                "uiTheme": "vs-dark",
                "path": "./themes/protanopia-deuteranopia.json"
            },
            {
                "label": "Slack Theme Tritanopia",
                "uiTheme": "vs-dark",
                "path": "./themes/tritanopia.json"
            },
            {
                "label": "Slack Theme Dark Mode",
                "uiTheme": "vs-dark",
                "path": "./themes/dark-mode.json"
            }
        ]
    },
    "dependencies": {
        "vsce": "^1.87.0"
    }
}
